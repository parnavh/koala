{
  "name": "koala",
  "version": "0.4.6",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./build && tsc-esm-fix --target='build'",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "tsx watch src/main.ts",
    "postinstall": "npm run db:gen",
    "start": "node ."
  },
  "dependencies": {
    "@changesets/cli": "^2.28.1",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@discordx/importer": "^1.3.1",
    "@discordx/pagination": "^3.5.6",
    "@discordx/utilities": "^7.0.3",
    "@google-cloud/text-to-speech": "^5.8.1",
    "@prisma/client": "6.5.0",
    "@t3-oss/env-core": "^0.7.1",
    "bullmq": "^5.43.1",
    "discord.js": "^14.18.0",
    "discordx": "^11.12.2",
    "ioredis": "^5.6.0",
    "prisma": "^6.5.0",
    "sodium-native": "^4.3.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2",
    "tscpaths": "^0.0.9",
    "tsx": "^4.19.3",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  }
}
